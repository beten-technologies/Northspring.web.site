@{
    if (ViewBag.Title == "Pritam-Research")
    { ViewBag.Title = "Researches done by Pritam"; }
    else if (ViewBag.Title == "Chandran-Research")
    { ViewBag.Title = "Researches done by Chandran"; }
    else if (ViewBag.Title == "Chandran")
    { ViewBag.Title = "Chandran Gnanamuthu"; }
    else if (ViewBag.Title == "Pritam")
    { ViewBag.Title = "Pritam Majumdar"; }
}
    <section id="blogArchive">
        <div class="row">
            <div class="col-lg-12 col-md-12">
                <div class="blog-breadcrumbs-area">
                    <div class="container">
                        <div class="blog-breadcrumbs-left">
                            <h2>@ViewBag.Title</h2>
                        </div>
                        <div class="blog-breadcrumbs-right">
                            <ol class="breadcrumb">
                                <li>You are here</li>
                                <li><a href="~/Home/Index">Home</a></li>
                                <li class="active">@ViewBag.Title</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    @{
        string partialPageName = ViewBag.PartialPage;
        @Html.Partial(partialPageName);
    }
